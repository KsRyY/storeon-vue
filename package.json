{
  "name": "storeon-vue",
  "version": "0.0.1",
  "description": "A tiny connector for Storeon and Vue",
  "main": "index.js",
  "scripts": {
    "start": "parcel serve example/index.html --open",
    "lint": "eslint ./*.js",
    "test": "yarn lint && size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distolma/storeon-vue.git"
  },
  "author": "Dmytro Mostovyi <djmakaron2009@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/distolma/storeon-vue/issues"
  },
  "homepage": "https://github.com/distolma/storeon-vue#readme",
  "devDependencies": {
    "@logux/eslint-config": "^35.0.1",
    "@size-limit/preset-small-lib": "^2.2.3",
    "@vue/component-compiler-utils": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-es5": "^1.4.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^14.0.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "parcel-bundler": "^1.12.4",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "storeon": "^0.9.7"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "size-limit": [
    {
      "path": "index.js",
      "limit": "158 B"
    }
  ],
  "eslintConfig": {
    "extends": "@logux/eslint-config/browser"
  },
  "eslintIgnore": [
    "node_modules",
    "example",
    "index.test.js"
  ],
  "dependencies": {
    "storeon": "^0.9.7",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4"
  }
}
